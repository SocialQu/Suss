{
  "compilerOptions": {
    "resolveJsonModule": true,
    "downlevelIteration": true,

    "target": "es5",
    "module": "commonjs",

    "strict": true,
    "esModuleInterop": true,

    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,

    "lib": ["es2019", "DOM"],
    "types": ["node"]
  },
}
